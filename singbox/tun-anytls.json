{
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "tag": "google",
        "address": "8.8.8.8"  
      }
    ]
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "sing-tun",
      "sniff": true,
      "address": [
        "10.254.0.1/24",
        "fd00::1/64"
      ],
      "auto_route": true,
      "strict_route": true
    }
  ],
  "outbounds": [
    {
      "type": "anytls",
      "tag": "anytls-out",
      "server": "123.123.123.123", // 服务端地址
      "server_port": 8443, // 节点端口
      "password": "AAAaaa123123123", // 协议认证密码
      "tls": {
        "enabled": true,
        "insecure": true, // 允许不安全
        "server_name": "bing.com"
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "rules": [
      {
        "protocol": "dns",
        "outbound": "direct"
      },
      {
        "ip_version": 6,
        "outbound": "anytls-out" // IPV6走代理，需要节点有IPV6
      },
      {
        "inbound": ["tun-in"],
        "network": "tcp",
        "port": [22],
        "outbound": "direct"
      },
      {
        "ip_cidr": [
          "192.168.0.0/16",
          "127.0.0.1/32",
          "fc00::/7"
        ],
        "outbound": "direct"
      }
    ],
    "final": "anytls-out"
  }
}