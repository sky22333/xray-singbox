{
  "log": {
    "level": "info"
  },
  "dns": {
    "servers": [
      {
        "address": "tls://8.8.8.8",
        "strategy": "ipv4_only"
      }
    ]
  },
  "inbounds": [
    {
      "type": "hysteria2",
      "listen": "::",
      "listen_port": 8443, // 节点端口
      "up_mbps": 300,
      "down_mbps": 300,
      "users": [
        {
          "password": "example123" // 认证密码
        }
      ],
      "tls": {
        "enabled": true,
        "alpn": [
            "h3"
        ],
        "acme": {
          "domain": [
            "example.com" // TLS域名，通过内置的acme自动申请证书，无需手动干预，需要临时占用80端口
          ],
          "data_directory": "/etc/sing-box",
          "default_server_name": "example.com",  // 你的域名
          "email": "123456789admin@gmail.com"  // 申请证书的邮箱，可随意填写
        }
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      }
    ]
  }
}
