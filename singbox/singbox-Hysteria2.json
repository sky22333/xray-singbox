{
    "inbounds": [
        {
            "type": "hysteria2",
            "listen": "::",
            "listen_port": 8443, // 节点端口
            "up_mbps": 300,
            "down_mbps": 300,
            "users": [
                {
                    "password": "example123" // 认证密码
                }
            ],
            "tls": {
                "enabled": true,
                "alpn": [
                    "h3"
                ],
                "certificate_path": "/root/fullchain.cer", // 证书路径
                "key_path": "/root/private.key" // 私钥路径
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct"
        }
    ]
}
