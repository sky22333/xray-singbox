{
  "log": {
    "level": "info"
  },
  "inbounds": [
    {
      "type": "hysteria2",
      "listen": "::",
      "listen_port": 8443,
      "up_mbps": 300,
      "down_mbps": 300,
      "users": [
        {
          "password": "example123"
        }
      ],
      "tls": {
        "enabled": true,
        "insecure": true,
        "server_name": "bing.com",
        "certificate_path": "/etc/sing-box/cert.pem",
        "key_path": "/etc/sing-box/key.pem"
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct"
    }
  ]
}
