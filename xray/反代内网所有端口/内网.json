{
  "reverse": {
    "bridges": [
      {
        "tag": "bridge",
        "domain": "your-public-server-ip" // 替换为您的公网服务器IP
      }
    ]
  },
  "inbounds": [
    {
      "tag": "reverse",
      "port": 12955, // 与公网服务器的tunnel入站端口相同
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1",
        "port": 0, // 0表示任意端口
        "network": "tcp,udp"
      }
    }
  ],
  "outbounds": [
    {
      "tag": "tunnel",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "your-public-server-ip", // 替换为您的公网服务器IP
            "port": 44721,
            "users": [
              {
                "id": "8f91b6a0-e8ee-11ed-a05b-0242ac120003",
                "alterId": 0
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp"
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "inboundTag": ["bridge"],
        "outboundTag": "tunnel"
      },
      {
        "type": "field",
        "inboundTag": ["reverse"],
        "outboundTag": "direct"
      }
    ]
  }
}
