{
  // 反向代理设置
  "reverse": {
    // 桥接设置，用于连接到公网服务器
    "bridges": [
      {
        "tag": "bridge",
        "domain": "your-public-server-ip" // 替换为您的公网服务器IP
      }
    ]
  },
  // 出站连接设置
  "outbounds": [
    {
      "tag": "tunnel",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "your-public-server-ip", // 替换为您的公网服务器IP
            "port": 44721, // 随机生成的端口，与公网服务器入站端口相同
            "users": [
              {
                "id": "8f91b6a0-e8ee-11ed-a05b-0242ac120003", // 随机生成的UUID
                "alterId": 0
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp"
      }
    },
    // 默认出站设置，用于直接访问互联网
    {
      "tag": "direct",
      "protocol": "freedom"
    }
  ],
  // 路由规则设置
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "inboundTag": ["bridge"],
        "outboundTag": "tunnel"
      },
      {
        "type": "field",
        "network": "udp,tcp",
        "outboundTag": "direct"
      }
    ]
  }
}
