{
  // 入站连接设置
  "inbounds": [
    {
      "tag": "external",
      "port": 44721, // 随机生成的端口，用于接收来自手机的连接
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "8f91b6a0-e8ee-11ed-a05b-0242ac120003", // 随机生成的UUID，与家庭网络配置中的相同
            "alterId": 0
          }
        ]
      },
      "streamSettings": {
        "network": "tcp"
      }
    },
    {
      "tag": "tunnel",
      "port": 12955, // 随机生成的端口，用于接收来自家庭网络的反向连接
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "9d85be10-e8ee-11ed-a05b-0242ac120003", // 随机生成的UUID，用于家庭网络反向连接
            "alterId": 0
          }
        ]
      },
      "streamSettings": {
        "network": "tcp"
      }
    }
  ],
  // 出站连接设置
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom"
    }
  ],
  // 路由规则设置
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "inboundTag": ["external"],
        "outboundTag": "tunnel"
      },
      {
        "type": "field",
        "network": "udp,tcp",
        "outboundTag": "direct"
      }
    ]
  }
}
